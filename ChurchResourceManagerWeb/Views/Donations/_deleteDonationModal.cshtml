<!--header-->
<div class="modal-header">
    <button type="button" class="close" ng-click="ddc.dismiss()">&times;</button>
    <h4>Delete Transaction</h4>
</div>
<!--Body-->
<div class="modal-body">
    <h4>Are you sure you want to delete?</h4>
</div>
<!--Footer-->
<div class="modal-footer">
    <button class="btn btn-danger" type="submit" ng-click="ddc.deleteDonation()">Delete</button>
    <button class="btn btn-info" type="button" ng-click="ddc.closeModal()">Cancel</button>
</div>