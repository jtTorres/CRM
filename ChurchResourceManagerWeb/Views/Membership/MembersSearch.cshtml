<page-header page-title="Members Search"></page-header>

<div ng-controller="membershipController as mc">
    <div ng-controller="membershipReportingController as mrc">
        <reporting-tabs report-type="Membership" go-button="mrc.getMembership(searchType, startDate, endDate, familyId, memberId)"
                        no-results-found="mrc.noResults" reset-tabs="mrc.reset()" selected-search-type="MembershipDate"
                        invalid-report-type="mrc.invalidReportType" active-tab="0">
        </reporting-tabs>

        <members-grid open-edit-modal="mc.openEditMembershipModal(member, membership)" get-member-search-data="mc.getMemberSearch()" members="mrc.members" show-edit-delete="true"></members-grid>
    </div>
</div>

